<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cuit.model.UserBean" table="foragOwner.usertab">
        <id name="utId">
            <column name="utId"/>
        </id>
        <property name="utName">
            <column name="utName"/>
        </property>
        <property name="utPass">
            <column name="utPass"/>
        </property>
        <property name="utFeatureUri">
            <column name="utFeatureUri"/>
        </property>
        <property name="utMail">
            <column name="utMail"/>
        </property>
        <property name="utDate">
            <column name="utDate"/>
        </property>
        <property name="utAddr">
            <column name="utAddr"/>
        </property>
        <property name="utPro">
            <column name="utPro"/>
        </property>
        <property name="utEdu">
            <column name="utEdu"/>
        </property>
        <property name="utIntro">
            <column name="utIntro"/>
        </property>
        <property name="utSkill">
            <column name="utSkill"/>
        </property>
        <property name="uPic">
            <column name="uPic"/>
        </property>
    </class>
</hibernate-mapping>