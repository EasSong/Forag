<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cuit.model.MessageBean" table="foragOwner.MsgTable">
        <id name="mId">
            <column name="mid"/>
        </id>
        <property name="mSource">
            <column name="mSource"/>
        </property>
        <property name="mTitle">
            <column name="mTitle"/>
        </property>
        <property name="mIntro">
            <column name="mIntro"/>
        </property>
        <property name="mPic">
            <column name="mPic"/>
        </property>
        <property name="mTags">
            <column name="mTags"/>
        </property>
        <property name="mAuthor">
            <column name="mAuthor"/>
        </property>
        <property name="mContent">
            <column name="mContent"/>
        </property>
        <property name="mPublishTime">
            <column name="mPublishTime"/>
        </property>
        <property name="mCollectTime">
            <column name="mCollectTime"/>
        </property>
        <property name="mLikeCount">
            <column name="mLikeCount"/>
        </property>
        <property name="mDislikeCount">
            <column name="mDislikeCount"/>
        </property>
        <property name="mCollectCount">
            <column name="mCollectCount"/>
        </property>
        <property name="mTransmitCount">
            <column name="mTransmitCount"/>
        </property>
    </class>
</hibernate-mapping>